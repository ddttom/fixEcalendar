{
  "name": "fixecalendar",
  "version": "1.0.0",
  "description": "Convert calendar entries from Microsoft Outlook PST files to iCalendar (iCal/ICS) format",
  "main": "dist/index.js",
  "bin": {
    "fixECalendar": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "pst",
    "ical",
    "ics",
    "calendar",
    "outlook",
    "converter",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "pst-extractor": "^1.7.3",
    "ical-generator": "^7.2.0",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "better-sqlite3": "^11.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/better-sqlite3": "^7.6.12",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
